#include <stdio.h>
 int main()
 {
    char m, n;
    m = 'A';
    n = 65;


    printf("m 값 출력 : %c \n",m);
    printf("n 값 출력 : %c \n", n);
    printf("ASCII 코드 65 값 출력 : %c \n", 65);
    printf("ASCII 코드 65+1 값 출력 : %c \n", 65+1);
    printf("m+1 값 출력 : %c \n", m+1);
    printf("n+1 값 출력 : %c \n", n+1);

    return 0;
    }
